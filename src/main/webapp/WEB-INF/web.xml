<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <display-name>Archetype Created Web Application</display-name>
    <welcome-file-list>
        <welcome-file>/</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>registrationServlet</servlet-name>
        <servlet-class>com.emelib.servlets.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.emelib.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bookServlet</servlet-name>
        <servlet-class>com.emelib.servlets.BookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mainServlet</servlet-name>
        <servlet-class>com.emelib.servlets.MainServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signInUp</servlet-name>
        <servlet-class>com.emelib.servlets.SignInUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>exit</servlet-name>
        <servlet-class>com.emelib.servlets.ExitServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>catalog</servlet-name>
        <servlet-class>com.emelib.servlets.CatalogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>personalArea</servlet-name>
        <servlet-class>com.emelib.servlets.PersonalAreaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contract</servlet-name>
        <servlet-class>com.emelib.servlets.ContractServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>trade</servlet-name>
        <servlet-class>com.emelib.servlets.TradeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>booktext</servlet-name>
        <servlet-class>com.emelib.servlets.BookTextServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>booktext</servlet-name>
        <url-pattern>/bookText</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>trade</servlet-name>
        <url-pattern>/trade</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contract</servlet-name>
        <url-pattern>/contract</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrationServlet</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bookServlet</servlet-name>
        <url-pattern>/book</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signInUp</servlet-name>
        <url-pattern>/signInUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>exit</servlet-name>
        <url-pattern>/exit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>catalog</servlet-name>
        <url-pattern>/catalog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>personalArea</servlet-name>
        <url-pattern>/personalArea</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mainServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.png</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.jpg</url-pattern>
    </servlet-mapping>
</web-app>
